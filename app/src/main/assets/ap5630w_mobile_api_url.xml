<?xml version="1.0" encoding="UTF-8"?>
<service
    name="AP5630W RESTful API">

    <servers>
        <server
            env="dev"
            scheme="http://"
            host="cap.askey.com"
            port="4422"  />
    </servers>

    <apis>

